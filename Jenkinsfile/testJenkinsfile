pipeline{
    agent any
    parameters { string(name: 'DEPLOY_ENV', defaultValue: 'staging', description: '') }
    environment{
    	name = 'andy'
    	sex = 'male'
    }
    options { timestamps() }
    tools {
        maven 'apache-maven-3.6.0' 
    }
    stages{
        stage('print env'){
        	environment{
        		SSH_KEY = credentials('gitaccount')
        	}
	
            steps {
              script{
	      	  def rootDir = pwd()
                  def common = load "${rootDir}/common.Groovy"
                  common.testFunc()
              }
            	echo "${DEPLOY_ENV}"
            	sh 'printenv'
		sh 'mvn --version'
            }
        }
    }
    post{
    	failure{
    		echo "$name test Failed!"
    	}
    	success{
    		echo "$name test PASS!"
    	}
    }
}
